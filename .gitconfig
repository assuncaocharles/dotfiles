[user]
  name = Charles Assuncao
  email = junioassuncaocharles@gmail.com
[pager]
  branch = false
  log = false
[pull]
  rebase = true
[branch]
  autosetuprebase = always
[alias]
  gw = switch
  gs = status
  gp = push
  gc = commit -m 
  ga = add .
  cp = cherry-pick
  amend = commit -a --amend --no-edit
  unstage = reset --soft HEAD
[push]
  default = current
[core]
  pager = delta --theme="night-owlish"
[interactive]
    diffFilter = delta --color-only
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
